                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="items" maxOccurs="unbounded" minOccurs="0">
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:string">
                      <xs:attribute type="xs:int" name="itemTemplateId" use="optional"/>
                      <xs:attribute type="xs:int" name="itemColor" use="optional"/>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute type="xs:string" name="name" use="optional"/>
            <xs:attribute type="xs:string" name="gender" use="optional"/>
            <xs:attribute type="xs:string" name="race" use="optional"/>
            <xs:attribute type="xs:string" name="class" use="optional"/>
