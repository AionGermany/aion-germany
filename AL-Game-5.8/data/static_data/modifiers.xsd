    <xs:simpleType name="LeftHandSlot">
        <xs:restriction base="xs:string">
            <xs:enumeration value="SHIELD"/>
            <xs:enumeration value="DUAL"/>
        </xs:restriction>
    </xs:simpleType>
</xs:schema>