			<part name="sessionId" type="d" />
			<part name="?" type="b" size="27" />
		</packet>
		<packet id="0x08" name="CM_UPDATE_SESSION" >
			<part name="accountId" type="d" />
			<part name="loginOk" type="d" />
			<part name="reconnectKey" type="d" />
			<part name="garbage" type="b" size="0xb" />
		</packet>
	</packetfamilly>
</protocol>
